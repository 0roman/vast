namespace vast.detail;

enum DataType : ubyte {
  NoneType,
  BooleanType,
  IntegerType,
  CountType,
  RealType,
  TimestampType,
  TimespanType,
  EnumerationType,
  StringType,
  PatternType,
  AddressType,
  SubnetType,
  PortType,
  VectorType,
  SetType,
  MapType
}

table MapEntry {
  key:Data;
  value:Data;
}

table Data {
  which:DataType = NoneType;
  integer:long = 0;
  count:ulong = 0;
  real:double = 0.0;
  bytes:[ubyte];
  vector:[Data];
  map:[MapEntry];
}

root_type Data;
